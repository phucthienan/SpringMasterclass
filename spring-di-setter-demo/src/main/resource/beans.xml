<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="organization" class="com.nguyenphucthienan.Organization">
        <constructor-arg name="companyName" value="Spring"></constructor-arg>
        <constructor-arg name="yearOfIncorporation" value="2018"></constructor-arg>
        <property name="postalCode" value="88000"/>
        <property name="employeeCount">
            <value>9000</value>
        </property>
    </bean>
</beans>