<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="firstOrganization" class="com.nguyenphucthienan.Organization">
        <constructor-arg index="1" value="2018"></constructor-arg>
        <constructor-arg index="0" value="Spring"></constructor-arg>
    </bean>

    <bean id="secondOrganization" class="com.nguyenphucthienan.Organization">
        <constructor-arg name="companyName" value="Spring"></constructor-arg>
        <constructor-arg name="yearOfIncorporation" value="2018"></constructor-arg>
    </bean>
</beans>